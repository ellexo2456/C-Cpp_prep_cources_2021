TARGET = test

SRCS = \
	project/src/main.c\
	project/src/master_write.c \
	project/src/update_black_record.c \
	project/src/transaction_write.c 

OBJS = $(SRCS:.c=.o)

.PHONY: all

all: $(TARGET)

$(TARGET): $(OBJS)
	$(CC) -o $(TARGET) $(OBJS)

.c.o:
	$(CC) -c $< -o $@

clean:
	rm -rf $(TARGET) $(OBJS)
